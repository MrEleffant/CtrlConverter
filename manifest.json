{
    "name": "CtrlConverter",
    "description": "This is a Chrome extension that replace your copypaste from db",
    "version": "3.0.0",
    "manifest_version": 2,
    "permissions": [
      "tabs",
      "activeTab",
      "storage"
    ],
    "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png" },
    "browser_action": {
      "default_title": "CtrlConverter",  
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ]
}
