{
    "name": "CtrlConverter",
    "description": "Extension liée au serveur discord DbCreator, permettant de modifier le contenu du presse papier à partir d'une base de données.",
    "version": "3.0.3",
    "manifest_version": 3,
    "permissions": [
      "storage"
    ],
    "icons": { 
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png" },
    "action": {
      "default_title": "CtrlConverter",  
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ]
}
