{
    "name": "CtrlConverter",
    "description": "This is a Chrome extension !",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "index.js"
      },
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content-script.js"]
      }
    ]
}
